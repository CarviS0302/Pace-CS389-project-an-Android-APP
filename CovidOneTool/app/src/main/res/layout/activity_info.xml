<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/context">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="186dp"
        android:layout_marginTop="200dp"
        android:background="@color/title"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/title"
            android:text="Developers" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/developerName1"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="@color/title"
                android:fontFamily="@font/graphiksemibold"
                android:gravity="center"
                android:text="Kai Wang" />

            <EditText
                android:id="@+id/developerEmailAddress1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/title"
                android:editable="false"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:textColor="#03A9F4"
                tools:text=" kw87502p@pace.edu" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/developerName2"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="@color/title"
                android:fontFamily="@font/graphiksemibold"
                android:gravity="center"
                android:text="Houqi Zhan" />

            <EditText
                android:id="@+id/developerEmailAddress2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/title"
                android:editable="false"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:textColor="#03A9F4"
                tools:text=" hz865689p@pace.edu" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/developerName3"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="@color/title"
                android:fontFamily="@font/graphiksemibold"
                android:gravity="center"
                android:text="Bisheng Zeng" />

            <EditText
                android:id="@+id/developerEmailAddress3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/title"
                android:editable="false"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:textColor="#03A9F4"
                tools:text=" bz92714p@pace.edu" />
        </LinearLayout>


    </LinearLayout>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Covid_OneTool"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="146dp"
        android:layout_height="133dp"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

</androidx.constraintlayout.widget.ConstraintLayout>